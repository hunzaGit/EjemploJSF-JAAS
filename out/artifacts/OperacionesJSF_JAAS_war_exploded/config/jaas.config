RanchLogin {
  
   com.javaranch.auth.RanchLoginModule required; 

};


SencilloLogin {
	simple.seguridad.SencilloLoginModule required;
};


SumaAuthorization {
	sumaAuthorization.presentacion.seguridad.SumaAuthorizationLoginModule required;
};


Operaciones {
	operaciones.presentacion.seguridad.OperacionesLoginModule required;
};

SumaAuthorizationControlador{
	sumaAuthorizationControlador.presentacion.seguridad.SumaAuthorizationControladorLoginModule required;
};

SSO {
	sso1.presentacion.seguridad.SSO1LoginModule required useSharedState=true;
	sso1.presentacion.seguridad.SSO2LoginModule required useSharedState=true;
};

OperacionesFilter {
	operacionesFilter.presentacion.seguridad.OperacionesLoginModule required;
};
